    "import numpy as np\n",
    "\n",
    "# Define the input signal x[n] and impulse response h[n]\n",
    "x = [0, -1, -1.5, 2, 1.5, 1.5, 0.8, 0, -0.5]\n",
    "h = [1, -0.8, -0.5, -0.2]\n",
    "\n",
    "# Perform convolution\n",
    "y = np.convolve(x, h)\n",
    "\n",
    "# Print the result\n",
    "print(\"Output y[n]:\", y)\n"
   ]
  }
